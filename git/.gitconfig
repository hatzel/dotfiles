[user]
    name = Hans Ole Hatzel
    email = hhatzel@gmail.com
[push]
    default = simple
[core]
    editor = vim
[color]
    ui = true
[alias]
    lg = log --graph --all --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
    ls = status
    ch = checkout
